
<div class="row">
    <div class="col-md-4">
        <div class="form-group">
            <label>First Name</label>
            <input type="text" id="txtfirstName" class="form-control input-sm" name="Hotel.firstName" placeholder="Enter First Name">
        </div>
    </div>
    <div class="col-md-4">
        <div class="form-group">
            <label>Middle Name</label>
            <input type="text" id="txtmiddleName" class="form-control input-sm" name="Hotel.middleName" placeholder="Enter Middle Name">
        </div>
    </div>
    <div class="col-md-4">
        <div class="form-group">
            <label>Last Name</label>
            <input type="text" id="txtlastName" class="form-control input-sm" name="Hotel.lastName" placeholder="Enter Last Name">
        </div>
    </div>
</div>


<div class="row">
    <div class="col-md-4">
        <div class="form-group">
            <label>Gender</label>
            <select id="drpgender" class="form-control input-sm" name="Hotel.gender">
                <option value="">-Select Gender-</option>
            </select>
        </div>
    </div>
    <div class="col-md-4">
        <div class="form-group">
            <label>Date Of Birth</label>
            <input type="text" id="dtpdateOfBirth" class="form-control datepicker" name="Hotel.dateOfBirth">
        </div>
    </div>
    <div class="col-md-4">
        <div class="form-group">
            <label>Email Address</label>
            <input type="text" id="txtemailAddress" class="form-control input-sm" name="Hotel.emailAddress" placeholder="Enter Email Address">
        </div>
    </div>
</div>


<div class="row">
    <div class="col-md-4">
        <div class="form-group">
            <label>Phone Number</label>
            <input type="text" id="txtphoneNumber" class="form-control input-sm" name="Hotel.phoneNumber" placeholder="Enter Phone Number">
        </div>
    </div>
    <div class="col-md-4">
        <div class="form-group">
            <label>Mobile Number</label>
            <input type="text" id="txtmobileNumber" class="form-control input-sm" name="Hotel.mobileNumber" placeholder="Enter Mobile Number">
        </div>
    </div>
    <div class="col-md-4">
        <div class="form-group">
            <label>Address</label>
            <textarea class="form-control input-sm" name="Hotel.address" placeholder="Enter Address" maxlength="250"></textarea>
        </div>
    </div>
</div>




