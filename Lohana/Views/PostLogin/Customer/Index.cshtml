

<div class="content-wrapper">

    @{Html.RenderPartial("_ServerMessage");}

    <!-- Content Header (Page header) -->

    <section class="content-header">

        <h1>
            Customer

            <small>Create / Update Page</small>
        </h1>

        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i>Master</a></li>
            <li class="active">Customer</li>
        </ol>
    </section>

    <!-- Main content -->

    <section class="content">
        <div class="box">
            <div class="box-header">
                <h3 class="box-title">Customer</h3>
            </div>
            <div class="box-body">
               
                @{Html.RenderPartial("_CustomerInsert");}

                <div class="btn-group">
                    <button type="button" class="btn btn-success active" id="btnsave">Save</button>

                    <button type="reset" class="btn btn-warning active" id="btnreset">Reset</button>

                    <button type="button" class="btn btn-danger active" id="btnback">Cancel</button>
                </div>
            </div>
        </div>


    </section>

</div>
