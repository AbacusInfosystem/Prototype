

<!DOCTYPE html>

<html lang="en">

<head>
    <meta charset="utf-8" />

    <meta name="viewport" content="width=device-width" />

    <title>@ViewBag.Title</title>

    <link href="~/Content/AdminLTE/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />

    <link href="/Content/css/font-awesome.min.css" rel="stylesheet" />

    <link href="/Content/css/ionicons.min.css" rel="stylesheet" />

    <link href="/Content/AdminLTE/bootstrap/css/bootstrap.css" rel="stylesheet" />

    <link href="/Content/AdminLTE/bootstrap/js/bootstrap.js" rel="stylesheet" />

    <!-- Theme style -->
    <link href="~/Content/AdminLTE/dist/css/AdminLTE.min.css" rel="stylesheet" type="text/css" />

    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link href="~/Content/AdminLTE/dist/css/skins/_all-skins.min.css" rel="stylesheet" type="text/css" />
   
	 <!-- iCheck -->
    <link href="~/Content/AdminLTE/plugins/iCheck/skins/square/green.css" rel="stylesheet" />

    <!-- Date Picker -->
    <link href="~/Content/AdminLTE/plugins/datepicker/datepicker3.css" rel="stylesheet" type="text/css" />

    <!-- Time Picker -->
    <link href="~/Content/AdminLTE/plugins/timepicker/bootstrap-timepicker.min.css" rel="stylesheet" type="text/css" />

    <!-- jQuery 2.1.3 -->
    <script src="/Content/AdminLTE/plugins/jQuery/jQuery-2.1.3.min.js"></script>
    <script src="/Content/AdminLTE/plugins/jQueryUI/jquery-ui-1.10.3.min.js"></script>

    <script src="/Scripts/generic.ajax.js"></script>

    <script src="/Scripts/jquery.validate.js"></script>
    <script src="/Scripts/generic.custom.validation.js"></script>

    <script src="~/Scripts/generic.autocomplete.js"></script>
    <link href="~/Content/themes/base/jquery-ui.css" rel="stylesheet" />

    <script src="~/Scripts/generic.message.js"></script>
    <script src="~/Scripts/generic-methods.js"></script>
    

    <!-- Bootstrap 3.3.2 JS -->
    <script src="/Content/AdminLTE/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>

    <!-- datepicker -->
    <script src="/Content/AdminLTE/plugins/datepicker/bootstrap-datepicker.js" type="text/javascript"></script>
    
	<!-- timepicker -->
    <script src="/Content/AdminLTE/plugins/timepicker/bootstrap-timepicker.min.js" type="text/javascript"></script>
    
	<!-- Bootstrap WYSIHTML5 -->
    <script src="/Content/AdminLTE/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js" type="text/javascript"></script>
    
	<!-- AdminLTE App -->
    <script src="/Content/AdminLTE/dist/js/app.min.js" type="text/javascript"></script>
    
	<!-- iCheck -->
    <script src="/Content/AdminLTE/plugins/iCheck/icheck.js"></script>
    

</head>

<body class="skin-blue">

    <div class="wrapper">

        @{Html.RenderPartial("_Header");}

        @{Html.RenderPartial("_Sidebar");}

        @RenderBody()

        @{Html.RenderPartial("_Footer");}

    </div>

    <div class="modal fade" id="div_Parent_Modal_Fade" role="dialog" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header" id="div_Parent_Modal_Header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title">&nbsp;</h4>
                </div>
                <div class="modal-body" id="div_Parent_Modal_Body" style="z-index: 1150">

                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="div_Child_Modal_Fade" role="dialog" aria-hidden="true">
        <div class="modal-dialog ">
            <div class="modal-content">
                <div class="modal-header" id="div_Child_Modal_Header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title">&nbsp;</h4>
                </div>
                <div class="modal-body" id="div_Child_Modal_Body">

                </div>
            </div>
        </div>
    </div>

</body>

</html>

<script type="text/javascript">

    $(document).ready(function () {

        // get all checkboxes other than those which are added in .non-iCheck
        $('input:not(.non-iCheck input:checkbox)').iCheck({
            checkboxClass: 'icheckbox_square-green',
            radioClass: 'iradio_square-green',
            increaseArea: '20%' // optional
        });

        //$.getScript("/Content/AdminLTE/plugins/iCheck/icheck.js", function () {

        //    $("body").append(Initialize_ICheck());
        //});


    });
</script>